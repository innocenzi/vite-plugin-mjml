{
  "name": "vite-plugin-mjml",
  "version": "0.2.0",
  "author": "Enzo Innocenzi",
  "license": "MIT",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "*.d.ts"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/innocenzi/vite-plugin-mjml"
  },
  "homepage": "https://github.com/innocenzi/vite-plugin-mjml#readme",
  "bugs": "https://github.com/innocenzi/vite-plugin-mjml/issues",
  "scripts": {
    "prepare": "npm run build",
    "build": "unbuild",
    "test": "vitest",
    "lint": "eslint src/*.ts",
    "release": "bumpp --push --tag --commit \"release: v\""
  },
  "dependencies": {
    "debug": "^4.3.4",
    "fast-glob": "^3.2.11",
    "mjml": "^4.13.0",
    "mjml-core": "^4.13.0",
    "picocolors": "^1.0.0"
  },
  "devDependencies": {
    "@innocenzi/eslint-config": "^0.12.0",
    "@types/debug": "^4.1.7",
    "@types/mjml": "^4.7.0",
    "@types/mjml-core": "^4.7.1",
    "@types/node": "^18.0.4",
    "bumpp": "^8.2.1",
    "eslint": "^8.19.0",
    "typescript": "^4.7.4",
    "unbuild": "^0.7.4",
    "vite": "^3.0.0",
    "vitest": "^0.18.0"
